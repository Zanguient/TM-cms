// ID : Title ---> URL --> [Parent]
homepage                 : @(Homepage)               --> /
blogs                    : @(Blogs)                  --> /blogs/                      --> homepage
blogsdetail              : @(Blog detail)            --> /blogs/{0}/                  --> blogs
dynamic                  : @(Dynamic)                --> /pages/*                     --> homepage
//dynamic_lg               : @(Dynamic)                --> /{0}/pages/*                 --> homepage
